    <dependencies>
      <dependency id="FubuMVC.Core"/>
      <dependency id="FubuMVC.StructureMap"/>
      <dependency id="FubuMVC.Core.Assets"/>
	  <dependency id="FubuMVC.Core.UI"/>
	  <dependency id="FubuMVC.Core.View"/>
      <dependency id="FubuMVC.Diagnostics"/>
      <dependency id="WebActivator" version="1.1" />
    </dependencies>
  </metadata>
  <files>
    <file src="BootstrapFubu.cs.pp" target="content" />
	<file src="FubuAppStart.cs.pp" target="content\App_Start" />
    <file src="web.config.transform" target="content" />
  </files>
</package>
